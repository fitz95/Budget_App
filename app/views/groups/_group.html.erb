<div id="<%= dom_id group %>" class='group-flex'>
  <div class='icon-flex'>
  <div class='img-div'>
  <%= link_to(group, class: "img") do %>
    <%= image_tag(group.icon, class: "img-thumbnail", width: 85, height: 75) %>
  <% end %>
  </div>
 
  <div>
    <%= group.name %>
  </div>
  </div>
  <div>
    <h3 class='category-name'>$<%= @group_entity_totals[group.id] %></h3>
  </div>

</div>
